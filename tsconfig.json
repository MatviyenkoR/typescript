{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "types":[
      "jquery"
    ]
  },
    "exclude": [
      "node_modules",
      "dist",
      "./node_modules"
    ],
    "compileOnSave": false
}
